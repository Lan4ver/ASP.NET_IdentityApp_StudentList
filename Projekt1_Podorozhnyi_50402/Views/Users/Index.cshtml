@model IEnumerable<Projekt1_Podorozhnyi_50402.Models.User>

@{ 
    ViewBag.Title = "Users List";
}

<table class="table">
    <tr><th>Email</th><th>Year</th><th></th></tr>
    @foreach(var user in Model)
            {
                <tr>
                    <td>@user.Email</td>
                    <td>@user.Year</td>
                    <td>
                        <form method="post">
                            <a class="btn btn-sm btn-primary" asp-action="Open" asp-route-id="@user.Id">Open</a>                           
                        </form>
                    </td>
                </tr>
            }
</table>
